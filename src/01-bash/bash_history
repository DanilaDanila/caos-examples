SAVEHIST=[31m1000[m[19;58H1,1[11CTop[1;1H[?25h[?25l[19;1H[K[19;1H/[?2004h[?25hp[?25l[19;2H[K[19;2H[?25hPATH[?25l[27m[23m[29m[m[H[2J[1;1H[38;5;130mzstyle[m [31m':completion:*:kill:*'[m [35mcommand[m [31m'ps -u $USER -o pid,%cpu,tty,cputime,,[2;1Hcmd'[m


